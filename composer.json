    "keywords": [
        "php",
        "github",
        "stack",
        "languages"
    ],
    "homepage": "https://github.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Ilechukwu",
            "email": "francis94c@gmail.com",
            "homepage": "https://francis94c.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-curl": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "mikecao/flight": "^1.3"
    },
    "autoload": {
        "psr-0": {
            "GitHubStackProfiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
